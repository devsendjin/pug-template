{
  "name": "pug-template",
  "version": "1.0.0",
  "description": "Comfortable gulp template for creating html pages faster than usually",
  "main": "gulpfile.js",
  "scripts": {
    "dev:watch": "cross-env NODE_ENV=development gulp --s --o --unhandled-rejections=strict",
    "dev:build": "cross-env NODE_ENV=development gulp build --unhandled-rejections=strict",
    "prod:watch": "cross-env NODE_ENV=production gulp --s --o --unhandled-rejections=strict",
    "prod:build": "cross-env NODE_ENV=production gulp build --unhandled-rejections=strict"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@emitty/core": "^2.0.1",
    "@emitty/language-pug": "^2.0.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.10",
    "cross-env": "^7.0.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-html-beautify": "^1.0.1",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-remember": "^1.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "merge-stream": "^2.0.0",
    "postcss-inline-svg": "^4.1.0",
    "postcss-sort-media-queries": "^1.7.25",
    "terser-webpack-plugin": "^4.2.3",
    "through2": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-stream": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devsendjin/pug-template.git"
  },
