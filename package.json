{
  "name": "pug-template",
  "version": "2.0.0",
  "description": "Comfortable gulp template for creating html pages faster than usually",
  "main": "gulpfile.js",
  "scripts": {
    "dev-gulp-webpack-restart": "npm run clean && concurrently \"npm run dev-build-gulp\" \"npm run dev-build-webpack\" && concurrently \"npm run dev-watch-webpack\" \"npm run dev-server-gulp\"",
    "dev-gulp-webpack": "concurrently \"npm run dev-watch-webpack\" \"npm run dev-server-gulp\"",
    "dev-server-gulp": "gulp --serve --open",
    "dev-build-gulp": "gulp build",
    "prod-build-gulp": "cross-env NODE_ENV=production gulp build",
    "dev-watch-webpack": "node \"./webpack.config.js\" --watch",
    "dev-build-webpack": "node \"./webpack.config.js\" --build",
    "prod-build-webpack": "cross-env NODE_ENV=production node \"./webpack.config.js\" --build",
    "clean": "rm -rf \"./dist\""
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@emitty/core": "^2.0.1",
    "@emitty/language-pug": "^2.0.1",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.3.0",
    "browser-sync": "^2.27.5",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "dotenv": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-html-beautify": "^1.0.1",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-pug-3": "^1.2.2",
    "gulp-remember": "^1.0.1",
    "gulp-replace": "^1.1.3",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-terser": "^2.1.0",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.3.8",
    "postcss-inline-svg": "^5.0.0",
    "postcss-sort-media-queries": "^4.1.0",
    "postcss-sorting": "^6.0.0",
    "sass-loader": "^12.1.0",
    "terser-webpack-plugin": "^5.2.4",
    "through2": "^4.0.2",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpackbar": "^5.0.0-3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devsendjin/pug-template.git"
  },
  "keywords": [
    "pug",
    "gulp",
    "html"
  ],
  "browserslist": [
    "defaults"
  ],
  "author": "Anatoliy Skichko",
  "license": "ISC"
}
