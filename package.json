{
  "name": "pug-template",
  "version": "2.0.0",
  "description": "Comfortable gulp template for creating html pages faster than usually",
  "main": "gulpfile.js",
  "scripts": {
    "watch:dev:gulp": "cross-env NODE_ENV=development gulp --serve --open",
    "build:dev:gulp": "npm run clean && cross-env NODE_ENV=development gulp build",
    "build:prod:gulp": "npm run clean && cross-env NODE_ENV=production gulp build",
    "watch:dev:webpack": "cross-env NODE_ENV=development node \"./webpack.config.js\" --watch",
    "build:dev:webpack": "npm run clean && cross-env NODE_ENV=development node \"./webpack.config.js\" --build",
    "build:prod:webpack": "cross-env NODE_ENV=production node \"./webpack.config.js\" --build",
    "clean": "rm -rf \"./build\""
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@emitty/core": "^2.0.1",
    "@emitty/language-pug": "^2.0.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "dotenv": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-html-beautify": "^1.0.1",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-pug-3": "^1.2.1",
    "gulp-remember": "^1.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-terser": "^2.0.1",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-sort-media-queries": "^3.7.7",
    "postcss-sorting": "^5.0.1",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "through2": "^4.0.2",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpackbar": "^5.0.0-3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devsendjin/pug-template.git"
  },
  "keywords": [
    "pug",
    "gulp",
    "html"
  ],
  "browserslist": [
    "defaults"
  ],
  "author": "Anatoliy Skichko",
  "license": "ISC"
}
